{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint-config-egg",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "semi": ["error","always"],
    "indent":["error", 4],
    "camelcase": [1, {"properties": "always"}] ,
    "object-curly-spacing":["error", "never"]

  }
}
